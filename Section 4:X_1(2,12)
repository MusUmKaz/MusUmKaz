X_1(2,12)

R<x>:=PolynomialRing(Rationals());
f:=;
K<a>:= NumberField(f);
E := EllipticCurve([K |0, -1, 0, 1, 0]);
TorsionSubgroup(E);
Rank(E);
G, h := TorsionSubgroup(E);
torsion_pts_E := [ h(g) : g in G ];
torsion_pts_E;


K<x>:=PolynomialRing(Rationals());
F<a>:=NumberField();
u:=;
v:=;
q := v;
t := 4*v/(u*v^2-u-3*v^2-1);
E:=EllipticCurve([F|0,t^2-2*q*t-2,0,-(t^2-1)*(q*t+1)^2,0]);
E;
TorsionSubgroup(E);
