> R<x>:=PolynomialRing(Rationals());
> F<a>:=NumberField(x^2-x+2);
> x:=1/4*(-a + 2);
> y:= 1/8*(a - 6);
> X:=x;
> Y:=y+1;
> z:=1;
> U:=-X^2 - X*Y + X*z;
> V:=X*z + Y*z - z^2;
> W:=X^2;
> u:=U/W;
> v:=V/W;
> r:= 1-(u+v)/((v+1)*(u+v+1));
> s:=(1-u)/(v+1);
> E:=EllipticCurve([F|s-r*s+1,r*s-r^2*s,r*s-r^2*s,0,0]);
> E;
